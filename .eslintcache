 margin-left: auto;\r\n  font-size: 20px;\r\n`;\r\n\r\n\r\nexport default ({\r\n  id,\r\n  isSelf,\r\n  user: { username, avatar },\r\n  location,\r\n  files,\r\n  isLiked,\r\n  likeCount,\r\n  createdAt,\r\n  newComment,\r\n  caption,\r\n  currentItem,\r\n  toggleLike,\r\n  onKeyUp,\r\n  comments,\r\n  setSelfComments,\r\n  close\r\n}) => {\r\n  const [Copycaption, setCopycaption] = useState(caption);\r\n  const [deletePost, setDeletePost] = useState(true)\r\n  const [isLoader, setIsLoader] = useState(true)\r\n  if (!deletePost) {\r\n    close()\r\n  }\r\n  return (\r\n    <Div>\r\n      {isLoader ? null : <Loader />}\r\n      <Post>\r\n        <Files>\r\n          {files &&\r\n            files.map((file, index) => (\r\n              <File key={file.id} src={file.url} showing={index === currentItem} />\r\n            ))}\r\n        </Files>\r\n      </Post>\r\n      <PostComment>\r\n        <Meta>\r\n          <Header>\r\n            <Avatar size=\"sm\" url={avatar} />\r\n            <UserColumn>\r\n              <Link to={`/${username}`}>\r\n              \r\n              </Link>\r\n              <Location>{location}</Location>\r\n              <Caption><FatText text={username} /> {Copycaption}</Caption>\r\n            </UserColumn>\r\n\r\n            <MoreButton >\r\n              {isSelf ? <EditPopup id={id} setIsLoader={setIsLoader} setDeletePost={setDeletePost} setCopycaption={setCopycaption} Copycaption={Copycaption} /> : null}\r\n            </MoreButton>\r\n          </Header>\r\n\r\n          \r\n\r\n          {comments && (\r\n            <Comments className={\"commentsBox\"}>\r\n              {comments.map((comment, index) => (\r\n                <Comment key={comment.id} index={index}>\r\n\r\n                  <FatText text={comment.user.username} />\r\n                  <Div2>\r\n                    {comment.text}\r\n                  </Div2>\r\n                  <Div1>\r\n                    <Button>\r\n                      <MenuPopup setSelfCommen